<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.makemyaccounting.dashboard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.makemyaccounting.accounting.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.makemyaccounting.accounting.LoginActivity" />
        <activity android:name="com.makemyaccounting.accounting.RegistrationActivity" />
        <activity android:name="com.makemyaccounting.accounting.MainActivity" />
        <activity android:name="com.makemyaccounting.accounting.SignupActivity" />
        <activity android:name="com.makemyaccounting.accounting.Purchase" />
        <activity android:name="com.makemyaccounting.accounting.AddCustomerActivity" />
        <activity android:name="com.makemyaccounting.accounting.Add_item" />
        <activity android:name="com.makemyaccounting.accounting.SaleActivity" />
        <activity android:name="com.makemyaccounting.accounting.Debitnote" />
        <activity android:name="com.makemyaccounting.accounting.Creditnote"></activity>
    </application>

</manifest>